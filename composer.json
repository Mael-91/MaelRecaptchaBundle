{
    "name": "mael/recaptcha-bundle",
    "type": "symfony-bundle",
    "version": "1.0.0",
    "description": "Google Recaptcha integration for Symfony and Twig (easy integration with Symfony Form)",
    "keywords": ["google", "recaptcha", "captcha", "spam", "abuse", "twig", "symfony", "no-robot", "antispam"],
    "homepage": "https://github.com/Mael-91",
    "license": "MIT",
    "authors": [
        {
            "name": "Mael",
            "homepage": "https://mael-91.me/"
        },
        {
            "name": "Community Contributions",
            "homepage": "https://github.com/Mael-91/MaelRecaptchaBundle/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "google/recaptcha": "^1.2",
        "symfony/config": "^4.4|^5.0",
        "symfony/dependency-injection": "^4.4|^5.0",
        "symfony/form": "^4.4|^5.0",
        "symfony/http-foundation": "^4.4|^5.0.7",
        "symfony/http-kernel": "^4.4|^5.0",
        "symfony/options-resolver": "^4.4|^5.0",
        "symfony/twig-pack": "*",
        "symfony/validator": "^4.4|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "Mael\\MaelRecaptchaBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mael\\MaelRecaptchaBundle\\Tests\\": "tests/"
        }
    }
}
